pipeline {
    agent any

    stages {

        stage('Prepare Artifacts') {
            steps {
                sh '''
                cd static 
                zip -r ../frontend-1.zip *
                '''         
            }

        }
        stage('Upload Artifacts') {
          steps {
             sh '''
               curl -f -v -u admin:DevOps321 --upload-file frontend-1.zip http://172.31.13.99:8081/repository/frontend-1/frontend-1.zip
             '''
    }
}

    }
}


